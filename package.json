{
  "name": "nami",
  "version": "0.1.0",
  "description": "A world generator",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "flood-fill": "parcel src/demos/flood-fill/index.html -d build",
    "meander": "parcel src/demos/meander/index.html -d build",
    "astar": "parcel src/demos/astar/index.html -d build",
    "dev": "parcel src/index.html -d build",
    "build": "parcel build src/index.html -d build --no-source-maps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hacktoon/nami.git"
  },
  "author": "Karlisson Bezerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hacktoon/nami/issues"
  },
  "homepage": "https://github.com/hacktoon/nami#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.8.6",
    "babel-preset-react": "^6.24.1",
    "lodash": "^4.17.15",
    "parcel-bundler": "^1.12.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "alias": {
    "lib": "./src/lib",
    "ui": "./src/ui",
    "model": "./src/model"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3"
  }
}
